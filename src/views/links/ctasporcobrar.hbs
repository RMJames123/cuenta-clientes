
        <table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="th-sm">Fecha</th>
                    <th class="th-sm">Documento</th>
                    <th class="th-sm">Cliente</th>
                    <th class="th-sm">Venta</th>
                    <th class="th-sm">Pago</th>
                    <th class="th-sm">Saldo</th>
                    <th class="th-sm">Vencimiento</th>
                    <th class="th-sm">Condicion</th>
                    <th class="th-sm">Telefonos</th>
                    <th class="th-sm">Zona</th>
                    <th class="th-sm">Direccion</th>
                </tr>
            </thead>
            <tbody>
                 {{#each ctasxcob}}
                <tr>
                    <td> {{ fecha }}</td>
                    <td> {{ documento }}</td>
                    <td> {{ cliente }}</td>
                    <td> {{ venta }}</td>
                    <td> {{ pagos }}</td>
                    <td> {{ saldo }}</td>
                    <td> {{ vencimto }}</td>
                    <td> {{ condicion }}</td>
                    <td> {{ telefonos }}</td>
                    <td> {{ zona }}</td>
                    <td> {{ direccion }}</td>
                </tr>
                 {{/each}}
            </tbody>
        </table>
   
 